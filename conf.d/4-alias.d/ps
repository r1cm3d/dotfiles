#alias pscpu="ps --user $(id -u) --sort -pcpu --format '%cpu %mem size fname pid' | head -n 10"
#alias psmem="ps --user $(id -u) --sort -size --format '%cpu %mem size fname pid' | head -n 10"
alias psmem="ps -mu $(id -u) -o rss,%mem,%cpu,pid,comm | head -n 15" 
alias pscpu="ps -ru $(id -u) -o rss,%mem,%cpu,pid,comm | head -n 15"
