alias ga='git add .'
alias gw='git switch'
alias gwn='git switch -c'
alias gb='git branch'
alias gc='git commit -s'
alias gca='git commit --amend'
alias gf='git fetch'
alias gs='git status -s'
alias gp='git push'
alias gpf='git push --force'
alias gd='git diff'
alias guf='git pull -X theirs origin master && git fetch'
alias gcb='git branch --show-current'
alias gpcb='git push --set-upstream origin $(git branch --show-current)'
alias gls='git log -n 1 --pretty=%s'
alias glb='git log -n 1 --pretty=%b'
